<%= form_for @cohort do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= label_tag :start_date %>
    <%= date_select :start_date, "start_date", default: @cohort.start_date %>

    <%= f.label :email %>
    <%= f.text_field :email %>

    <%= f.label :photos_url %>
    <%= f.text_field :photos_url %>

    <%= f.submit %>
<% end %>
